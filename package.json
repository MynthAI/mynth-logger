{
  "name": "mynth-logger",
  "version": "2.0.11",
  "description": "Package to format logs for mynth microservices.",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": "==24"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && pnpm tsc",
    "test": "pnpm vitest --run",
    "logs": "pnpm tsc && node dist/src/run.js",
    "prettier": "pnpm exec prettier -w '**/*.{js,jsx,ts,tsx,json,yml.j2,yml,yaml,.*}'",
    "lint": "concurrently \"pnpm exec prettier --check '**/*.{js,jsx,ts,tsx,json,yml.j2,yml,yaml,.*}'\" \"pnpm exec eslint . --max-warnings=0\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Mynth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com-mynth:MynthAI/mynth-logger.git"
  },
  "peerDependencies": {
    "arktype": "^2.1.29"
  },
  "dependencies": {
    "@ungap/structured-clone": "^1.3.0",
    "consola": "^3.4.2",
    "got": "^14.6.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/ungap__structured-clone": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.0",
    "eslint-plugin-ava": "^16.0.0",
    "eslint-plugin-file-extension-in-import-ts": "^2.1.1",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
